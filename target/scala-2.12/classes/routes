# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                          controllers.HomeController.diagram()

GET     /diagram                  controllers.HomeController.diagram()

GET    /diagram/update             controllers.HomeController.update()

GET    /diagram/fetchTest          controllers.HomeController.fetchTest()

GET    /diagram/formTest        controllers.HomeController.formTest()
# Map static resources from the /public folder to the /assets URL path
GET     /*file               controllers.Assets.versioned(path="/public", file: Asset)
